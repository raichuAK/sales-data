{
  "name": "project",
  "version": "1.0.0",
  "description": "to run express to return data",
  "main": "./routes/index.js",
  "type": "module",
  "scripts": {
    "serve": "node ./routes/index.js",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "test:api": "npm-run-all api test",
    "coverage": "jest --coverage",
    "lint:eslint": "eslint --ext .js . --ignore-path .gitignore",
    "format:eslint": "eslint --ext .js . --fix --ignore-path .gitignore",
    "lint:prettier": "prettier \"**/*.js\" --check --ignore-path .gitignore",
    "format:prettier": "prettier \"**/*.js\" --write --ignore-path .gitignore",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "format": "npm run format:eslint && npm run format:prettier",
    "dev": "npm-run-all serve",
    "api": "nodemon",
    "web": "web-dev-server --node-resolve --watch --app-index static/view.html --open",
    "migrate": "node ./migration/executor.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raichuAK/DisplayData.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/raichuAK/DisplayData/issues"
  },
  "homepage": "https://github.com/raichuAK/DisplayData#readme",
  "dependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "d3": "^4.0.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.2.1",
    "express": "^4.8.1",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.11",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "postgresql-client": "^1.16.7",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base"
    ],
    "overrides": [
      {
        "files": [
          "*"
        ],
        "rules": {
          "no-unused-expressions": "off",
          "babel/no-unused-expressions": "off",
          "import/no-extraneous-dependencies": "off",
          "class-methods-use-this": "off",
          "import/extensions": "off",
          "no-undef": "off",
          "no-console": "off",
          "no-underscore-dangle": "off",
          "no-param-reassign": "off"
        }
      }
    ],
    "globals": {
      "express": false
    }
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "routes/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@web/dev-server": "^0.1.18",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "jest": "^27.0.6",
    "jsdom": "^16.6.0",
    "koa-proxies": "^0.12.1",
    "supertest": "^6.1.3"
  }
}
